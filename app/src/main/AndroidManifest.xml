<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aetc.skip">

<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/skip_logo_cropped"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivitiesActivity" />
        <activity android:name=".JournalActivity" />
        <activity android:name=".RewardsActivity" />
        <activity android:name=".InboxActivity" />
        <activity android:name=".NewsActivity" />
        <activity android:name=".PostActivity"></activity>
    </application>

</manifest>